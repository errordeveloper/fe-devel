use Color;

function ColorKeyframe(Scalar time, Color value) {
  this.time = time;
  this.value = value;
}

function Color ColorKeyframe.interpolate(ColorKeyframe key2, Scalar t) {
  var Scalar u = (t - this.time) / (key2.time - this.time);
  return this.value + ((key2.value - this.value) * u);
}
