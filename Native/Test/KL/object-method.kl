struct Vec3
{
  Scalar x;
  Scalar y;
  Scalar z;
};

function Vec3.set( Scalar x, Scalar y, Scalar z )
{
  this.x = x;
  this.y = y;
  this.z = z;
}

operator entry()
{
  var Vec3 vec3;
  vec3.set( 1.0, 3.0, 5.0 );
  report(vec3);
}
