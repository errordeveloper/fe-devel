struct Vec3
{
  Scalar x;
  Scalar y;
  Scalar z;
};

function Vec3( Scalar x, Scalar y, Scalar z )
{
  this.x = x;
  this.y = y;
  this.z = z;
}

function Vec3.+=( Vec3 other )
{
  this.x += other.x;
  this.y += other.y;
  this.z += other.z;
}
operator entry()
{
  var Vec3 vec3( 1.0, 2.0, 3.0 );
  vec3 += vec3;
  report(vec3);
}
