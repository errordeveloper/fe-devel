q1.set(1.0,2.0,3.0,4.0);
v1.set(-1.0,-2.0,-3.0);
v2.set(4.0,5.0,6.0);
xfo1.set(v1,q1,v2);
xfo1.appendResult(tests, results, 'set');

xfo1.setIdentity();
xfo1.appendResult(tests, results, 'setIdentity');

v1.set(0.0,1.0,0.0);
q1.setFromAxisAndAngle(v1,1.570796326794896);
v1.set(0.0,0.0,10.0);
v2.set(2.0,2.0,2.0);
xfo1.set(v1,q1,v2);
xfo1.appendResult(tests, results, 'originalXfo');
m1 = xfo1.toMat44();
m1.appendResult(tests, results, 'toMat44');
v1.set(1.0,0.0,0.0);
m1.multiplyVector(v1).appendResult(tests, results, 'toMat44_multiplyVector');
xfo1.transformVector(v1).appendResult(tests, results, 'xfo1_transformVector');
res.setFromMat44(m1);
res.appendResult(tests, results, 'setFromMat44');
res.transformVector(v1).appendResult(tests, results, 'setFromMat44_transformVector');

v1.set(0.0,0.0,1.0);
q1.setFromAxisAndAngle(v1,-1.570796326794896);
v1.set(0.0,5.0,0.0);
v2.set(3.0,3.0,3.0);
xfo2.set(v1,q1,v2);
xfo2.appendResult(tests, results, 'originalXfo_2');
m2 = xfo2.toMat44();
m2.appendResult(tests, results, 'toMat44_2');
v1.set(1.0,0.0,0.0);
m2.multiplyVector(v1).appendResult(tests, results, 'toMat44_multiplyVector_2');
xfo2.transformVector(v1).appendResult(tests, results, 'xfo1_transformVector_2');
res.setFromMat44(m2);
res.appendResult(tests, results, 'setFromMat44_2');
res.transformVector(v1).appendResult(tests, results, 'setFromMat44_transformVector_2');

m2.multiplyInPlace(m1);
xfo2.multiplyInPlace(xfo1);
xfo2.appendResult(tests, results, 'multiply');
v1.set(1.0,0.0,0.0);
m2.multiplyVector(v1).appendResult(tests, results, 'toMat44_multiplyVector_3');
v1 = xfo2.transformVector(v1);
v1.appendResult(tests, results, 'xfo1_transformVector_3');

xfo1.set(xfo2.inverse());
xfo1.appendResult(tests, results, 'inverse');
v1 = xfo1.transformVector(v1);
v1.appendResult(tests, results, 'inverse_transformVector_3');


